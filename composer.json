{
    "name": "manuelkiessling/ai-tool-bridge",
    "description": "A library that simplifies making AIs talk to your own APIs",
    "license": "GPL-3.0-or-later",
    "type": "library",
    "keywords": [
        "chatgpt",
        "gpt-3",
        "gpt-4",
        "ai",
        "api",
        "json",
        "tool"
    ],
    "authors": [
        {
            "name": "Manuel Kie√üling",
            "email": "manuel@kiessling.net",
            "homepage": "https://manuel.kiessling.net"
        }
    ],
    "require": {
        "php": "^8.2",
        "ext-mbstring": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "ManuelKiessling\\AiToolBridge\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ManuelKiessling\\Test\\AiToolBridge\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
        },
        "sort-packages": true
    },
    "extra": {
    },
    "require-dev": {
        "phpunit/phpunit": "^10.1"
    }
}
